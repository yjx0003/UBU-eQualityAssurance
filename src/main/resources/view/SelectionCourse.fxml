<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="es.ubu.lsi.equalityassurance.controller.fxml.SelectionCourseController">
   <children>
      <TextField fx:id="textFieldSearch" promptText="%textField.search">
         <VBox.margin>
            <Insets bottom="20.0" right="20.0" top="20.0" />
         </VBox.margin>
      </TextField>
      <ListView fx:id="listViewCourses" VBox.vgrow="ALWAYS" />
   </children>
</VBox>
